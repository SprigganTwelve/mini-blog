{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
    {% for p in post %}
        <div
            class="col-sm-6"
        >
            <div class="card">
            <div class="card-body">
                <h5 class="card-title">{{p}}</h5>
                <p class="card-text">{{p}}.</p>
            </div>
            </div>
        </div>
    {% endfor %}
{% endblock %}
